<?xml version="1.0" encoding="utf-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Wikipedia-ja</ShortName>
  <Description>Wikipedia-ja</Description>
  <InputEncoding>utf-8</InputEncoding>
  <Image width="48" height="48">data:image/png;base64,</Image>
  <Url method="GET" type="text/html"
       template="https://ja.wikipedia.org/w/index.php">
    <Param name="search" value="{searchTerms}" />
    <Param name="fulltext" value="1" />
  </Url>
  <Url method="GET" type="application/x-suggestions+json"
       template="https://ja.wikipedia.org/w/api.php">
    <Param name="search" value="{searchTerms}" />
    <Param name="action" value="opensearch" />
    <Param name="namespace" value="0" />
    <Param name="limit"  value="10" />
    <Param name="format" value="json" />
  </Url>
  <moz:SearchForm>https://ja.wikipedia.org/w/index.php?search=</moz:SearchForm>
</OpenSearchDescription>
